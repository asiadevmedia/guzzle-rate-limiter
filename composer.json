{
    "name": "asiadevmedia/guzzle-rate-limiter",
    "description": "A rate limiter for Guzzle",
    "keywords": [
        "spatie",
        "asiadevmedia",
        "rate-limiter-middleware"
        "guzzle-rate-limiter-middleware"
    ],
    "homepage": "https://github.com/spatie/guzzle-rate-limiter-middleware",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastiandedeyne@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        },
        {
            "name": "Teguh Priyanto",
            "email": "asiadevmedia@gmail.com",
            "homepage": "https://teguhdevops.my.id",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1|^8.0",
        "guzzlehttp/guzzle": "^6.3|^7.0"
    },
    "require-dev": {
        "larapack/dd": "^1.0",
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Asiadevmedia\\GuzzleRateLimiter\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Asiadevmedia\\GuzzleRateLimiter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    }
}
